{
	"name": "NxRocksDevContainer",
	"image": "cirrusci/flutter:stable",
	"features": {
		"ghcr.io/devcontainers/features/java:1": {
			"version": "17",
			"installMaven": "true",
			"installGradle": "true"
		},
		"ghcr.io/devcontainers/features/node:1": {
			"version": "16"
