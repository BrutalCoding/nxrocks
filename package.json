{
  "name": "nxrocks",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "build": "nx run-many --target build --parallel 7 --exclude smoke",
    "test": "nx run-many --target test --parallel 7 --exclude smoke -- --runInBand",
    "e2e": "nx run-many --target e2e --parallel 7 --exclude smoke -- --runInBand",
    "lint": "nx workspace-lint && nx lint",
    "many": "nx run-many",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "migrate": "nx migrate latest",
    "ci": "pnpm install --frozen-lockfile",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "release": "DEBUG=true  nx run-many --target semantic-release --exclude=smoke --verbose",
    "workspace-generator": "nx workspace-generator",
    "prepare": "husky install",
    "smoke": "FORCE_SMOKE_TESTS=true yarn nx test smoke",
    "smoke-keep": "KEEP_SMOKE_TESTS_DIR=true yarn run smoke",
    "postinstall": "ts-node -P ./tools/tsconfig.tools.json ./tools/patch/index.ts"
  },
  "private": true,
  "dependencies": {
    "@swc/helpers": "0.5.1",
    "enquirer": "^2.3.6",
    "hpagent": "^1.2.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.9",
    "tslib": "^2.5.0",
    "unzipper": "^0.10.11",
    "xmlbuilder2": "^3.1.0",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@jest/globals": "^29.5.0",
    "@nx/devkit": "16.0.1",
    "@nx/eslint-plugin": "16.0.1",
    "@nx/jest": "16.0.1",
    "@nx/js": "16.0.1",
    "@nx/node": "16.0.1",
    "@nx/plugin": "16.0.1",
    "@nx/workspace": "16.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "0.1.62",
    "@swc/core": "^1.3.56",
    "@theunderscorer/nx-semantic-release": "^2.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "29.4.0",
    "@types/node": "16.11.7",
    "@types/node-fetch": "^2.6.3",
    "@types/tmp": "^0.2.3",
    "@types/unzipper": "^0.10.5",
    "@types/xmldoc": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "dotenv": "10.0.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "8.1.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.3",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-mock": "28.1.3",
    "jsonc-eslint-parser": "^2.2.0",
    "nx": "16.0.1",
    "nx-cloud": "16.0.5",
    "prettier": "2.6.2",
    "semantic-release": "^21.0.2",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tslint": "6.1.3",
    "typescript": "4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tinesoft/nxrocks.git"
  }
}
